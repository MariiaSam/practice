/*CHECKBOX*/



.modal-checkbox-form{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: -16px;
  margin-bottom: 24px;
  gap: 8px;
}

.checkbox-input{
outline: none;
}


.unchecked-icon{
  opacity: 1;
  stroke:  rgba(46, 47, 66, 0.4);
}
  
  .checked-icon{
  opacity: 0;
}
  
.checkbox-input:checked + .checkbox-icon > .unchecked-icon {
    opacity: 0;
}
  
.checkbox-input:checked + .checkbox-icon > .checked-icon {
    opacity: 1;
}


.checkbox-policy{
  display: block;
  font-family: var(--primary-font);
