  transition: fill 250ms cubic-bezier(0.4, 0, 0.2, 1);
}


.modal-name-input:focus + .modal-name-icon{
  fill: var(--iris-bgc-btn);
}

.modal-phone-box{
  position: relative;
  display: block;
  margin-bottom: 8px;
}

.modal-phone-icon{
  position: absolute;
  display: block;
  top: 50%;
  left: 19px;
  transform: translateY(-50%);
  fill: var(--secondary-bgc-color);
  transition: fill 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.modal-phone-input:focus + .modal-phone-icon{
  fill: var(--iris-bgc-btn);
}

.modal-email-box{
    position: relative;
    display: block;
    margin-bottom: 8px;
}

  .modal-email-icon{
    position: absolute;
